{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2021",
    "module": "commonjs",
    "strictNullChecks": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "esModuleInterop": true,
    "types": ["node"],
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
    }
  },
  // "include": [
  //   "src/**/*",
  //   "prisma/seed/**/*"
  // ]
}
